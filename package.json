{
  "name": "rails-mvp-starter",
  "private": true,
  "dependencies": {
    "@popperjs/core": "^2.10.2",
    "@rails/ujs": "^7.0.0-alpha2",
    "@tailwindcss/typography": "^0.4.1",
    "autoprefixer": "^10.3.7",
    "daisyui": "^1.14.2",
    "esbuild": "^0.13.4",
    "postcss": "^8.3.9",
    "sass": "^1.42.1",
    "tailwindcss": "^2.2.16"
  },
  "version": "0.1.0",
  "scripts": {
    "build": "esbuild app/javascript/*.* --bundle --outdir=app/assets/builds",
    "build:css": "sass ./app/assets/stylesheets/templates.scss ./app/assets/builds/templates.css --no-source-map --load-path=app/assets/stylesheets",
    "build:tailwind": "tailwindcss -i app/assets/stylesheets/application.tailwind.scss -o ./app/assets/builds/tailwind.css"
  },
  "engines": {
    "node": "12.16.2"
  }
}
